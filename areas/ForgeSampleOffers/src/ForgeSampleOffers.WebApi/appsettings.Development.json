{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Trace"
    },
    "ForgeLogger": {
      "Console": {
        "UseColors": true,
        "TimestampFormat": "HH:mm:ss",
        "Template": "{Timestamp} [{Level}] {Message}",
        "ShortenLevel": true
      }
    }
  }
}
